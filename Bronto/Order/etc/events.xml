<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="bronto_connector_queue_order_transform">
        <observer name="bronto_connector_queue_order_transform_handle" instance="Bronto\Order\Observer\AfterTransformEventObserver" />
    </event>
    <event name="bronto_connector_gather_endpoints">
        <observer name="bronto_order_gather_endpoints" instance="Bronto\Order\Observer\AfterGatherEndpointsObserver" />
    </event>
    <event name="bronto_connector_order_endpoint">
        <observer name="bronto_order_gather_endpoint" instance="Bronto\Order\Observer\AfterEndpointInfoObserver" />
    </event>
    <event name="bronto_connector_advanced_endpoint_additional">
        <observer name="bronto_order_advanced_import" instance="Bronto\Order\Observer\AfterAdvancedAdditionalObserver" />
    </event>
    <event name="bronto_connector_script_order_historical">
        <observer name="bronto_connector_script_order_historical" instance="Bronto\Order\Observer\AfterHistoricalImportObserver" />
    </event>
    <event name="bronto_connector_script_order_test">
        <observer name="bronto_connector_script_order_test" instance="Bronto\Order\Observer\AfterTestImportObserver" />
    </event>
    <event name="sales_order_save_after">
        <observer name="Bronto_Order::pushOrder" instance="Bronto\Order\Observer\AfterOrderSaveObserver" />
    </event>
    <event name="sales_order_place_after">
        <observer name="bronto_order_delete_tid" instance="Bronto\Order\Observer\AfterOrderPlacedObserver" />
    </event>
    <event name="bronto_email_template_filter">
        <observer name="bronto_order_email_template_filter" instance="Bronto\Order\Observer\AfterTemplateFilterObserver" />
    </event>
</config>
