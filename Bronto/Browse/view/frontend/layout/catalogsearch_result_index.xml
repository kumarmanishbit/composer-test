<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
  <body>
    <referenceContainer name="page.bottom">
      <block
        class="Bronto\Browse\CustomerData\Fiddle"
        name="bronto_browse_fiddle"
        template="Bronto_Browse::search.phtml">
        <arguments>
          <argument name="jsLayout" xsi:type="array">
            <item name="components" xsi:type="array">
              <item name="browse-fiddle" xsi:type="array">
                <item name="component" xsi:type="string">Bronto_Browse/js/fiddle</item>
              </item>
            </item>
          </argument>
        </arguments>
      </block>
    </referenceContainer>
  </body>
</page>
