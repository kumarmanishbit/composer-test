<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="bronto_connector_gather_endpoints">
        <observer name="bronto_email_gather_endpoints" instance="Bronto\Email\Observer\AfterGatherEndpointsObserver" />
    </event>
    <event name="bronto_connector_email_endpoint">
        <observer name="bronto_email_gather_endpoint" instance="Bronto\Email\Observer\AfterEndpointInfoObserver" />
    </event>
    <event name="bronto_connector_advanced_endpoint_additional">
        <observer name="bronto_email_advanced_import" instance="Bronto\Email\Observer\AfterAdvancedAdditionalObserver" />
    </event>
    <event name="bronto_connector_script_email_test">
        <observer name="bronto_connector_script_email_test" instance="Bronto\Email\Observer\AfterTestImportObserver" />
    </event>
    <event name="bronto_connector_script_email_historical">
        <observer name="bronto_connector_script_email_historical" instance="Bronto\Email\Observer\AfterHistoricalImportObserver" />
    </event>
    <event name="bronto_connector_trigger_flush">
        <observer name="bronto_connector_script_email_trigger_flush" instance="Bronto\Email\Observer\AfterTriggerFlushObserver" />
    </event>
    <event name="bronto_connector_script_email_event">
        <observer name="bronto_connector_script_email_event_handle" instance="Bronto\Email\Observer\AfterFlushMessagesObserver" />
    </event>
    <event name="bronto_connector_queue_delivery_transform">
        <observer name="bronto_email_queue_delivery_transform" instance="Bronto\Email\Observer\AfterTransformEventObserver" />
    </event>
    <event name="bronto_email_setting_sync_after">
        <observer name="bronto_email_setting_sync_after" instance="Bronto\Email\Observer\AfterSettingSyncObserver" />
    </event>
    <event name="wishlist_save_after">
        <observer name="bronto_email_wishlist_save_after" instance="Bronto\Email\Observer\Trigger\AfterWishlistSaveObserver" />
    </event>
    <event name="wishlist_delete_after">
        <observer name="bronto_email_wishlist_delete_after" instance="Bronto\Email\Observer\Trigger\AfterWishlistDeleteObserver" />
    </event>
    <event name="sales_model_service_quote_submit_success">
        <observer name="bronto_email_quote_submitted" instance="Bronto\Email\Observer\Trigger\AfterQuoteSaveObserver" />
        <observer name="bronto_email_order_placed" instance="Bronto\Email\Observer\Trigger\AfterOrderSaveObserver" />
    </event>
    <event name="sales_quote_delete_after">
        <observer name="bronto_email_quote_delete_after" instance="Bronto\Email\Observer\Trigger\AfterQuoteDeleteObserver" />
    </event>
    <event name="sales_order_save_after">
        <observer name="bronto_email_order_save_after" instance="Bronto\Email\Observer\Trigger\AfterOrderSaveObserver" />
    </event>
    <event name="sales_order_delete_after">
        <observer name="bronto_email_order_delete_after" instance="Bronto\Email\Observer\Trigger\AfterOrderDeleteObserver" />
    </event>
    <event name="bronto_connector_redirect_email">
        <observer name="bronto_email_redirect_path" instance="Bronto\Email\Observer\AfterRedirectPathObserver" />
    </event>
</config>
