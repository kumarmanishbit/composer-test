<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="bronto_connector_gather_endpoints">
        <observer name="bronto_advanced_gather_endpoints" instance="Bronto\Connector\Observer\AfterGatherEndpointsObserver" />
    </event>
    <event name="bronto_connector_advanced_endpoint">
        <observer name="bronto_advanced_gather_endpoint" instance="Bronto\Connector\Observer\AfterEndpointInfoObserver" />
    </event>
    <event name="bronto_connector_script_advanced_event">
        <observer name="bronto_connector_job_processevent_queue" instance="Bronto\Connector\Observer\AfterProcessEventsObserver" />
    </event>
    <event name="bronto_connector_script_advanced_test">
        <observer name="bronto_connector_script_process_test" instance="Bronto\Connector\Observer\AfterProcessTestObserver" />
    </event>
    <event name="bronto_connector_script_advanced_historical">
        <observer name="bronto_connector_script_historical_trigger" instance="Bronto\Connector\Observer\AfterProcessHistoricalObserver" />
    </event>
    <event name="bronto_connector_script_settings_test">
        <observer name="bronto_connector_script_settings_test_handle" instance="Bronto\Connector\Observer\AfterPreviewSettingsObserver" />
    </event>
    <event name="bronto_connector_trigger_flush">
        <observer name="bronto_connector_script_advanced_flush" instance="Bronto\Connector\Observer\AfterTriggerFlushObserver" />
    </event>
</config>
